@model IEnumerable<string>
<a class="btn btn-block btn-outline-secondary"href="/">Home</a>

@foreach (string category in Model) {
    string url = "/Catalog/Brand/" + category;
    <a class='btn btn-block  @(category == ViewBag.SelectedCategory? "btn-primary": "btn-outline-secondary")' href="@url">@category</a>
}